-- Scripts:

local function QIYWCDU_fake_script() -- TextLabel_2.Script 
	local script = Instance.new('Script', TextLabel_2)

	local GameName = game:GetService("MarketplaceService"):GetProductInfo(game.PlaceId).Name
	
	script.Parent.Text = GameName
end
coroutine.wrap(QIYWCDU_fake_script)()
local function GQRUQV_fake_script() -- Enter.Script 
	local script = Instance.new('Script', Enter)

	local keysystem = script.Parent.Parent.Key
	
	script.Parent.MouseButton1Click:Connect(function()
		if keysystem.Text == "SandyOp" then
			wait(0.5)
			keysystem.Text = "Correct"
			wait(1)
			pcall(function()
				spawn(function()
					for i, v in pairs(game.CoreGui:GetDescendants()) do
						if v.Name == "SANDYHUB" then
							v:Destroy()
						end
					end	
				end)
				spawn(function()
					loadstring(game:HttpGet("https://raw.githubusercontent.com/sandyonelite5103/sandy-hub/main/sandyhub%201"))()
				end)
			end)
		else
			keysystem.Text = "InCorrect"
			print("Fuck up")
		end
	end)
end
coroutine.wrap(GQRUQV_fake_script)()
local function GNTARDU_fake_script() -- Close.Script 
	local script = Instance.new('Script', Close)

	local function OnClicked()
		script.Parent.Parent.Parent:Destroy()
	end
	
	script.Parent.MouseButton1Click:connect(OnClicked)
end
coroutine.wrap(GNTARDU_fake_script)()
local function KAQI_fake_script() -- TextLabel_3.LocalScript 
	local script = Instance.new('LocalScript', TextLabel_3)

	script.Parent.Text = game.Players.LocalPlayer.Name
end
coroutine.wrap(KAQI_fake_script)()
local function JOCVYEH_fake_script() -- SANDYHUB.Script 
	local script = Instance.new('Script', SANDYHUB)

	frame = script.Parent.Frame
	frame.Draggable = true
	frame.Active = true
	frame.Selectable = true
end
coroutine.wrap(JOCVYEH_fake_script)()
